{
  "name": "starter-kit-client",
  "version": "1.0.0",
  "description": "Client side starter kit",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --mode development --config webpack.dev.config.js --port 8000 --progress --colors --open",
    "build": "webpack --mode production --config webpack.prod.config.js --progress --colors"
  },
  "jest": {
    "testURL": "http://localhost:8000",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cav2008/starter-kit-client.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Webpack",
    "Babel",
    "ES6",
    "Client",
    "Boilerplate",
    "Starter",
    "Kit"
  ],
  "author": "Cavan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cav2008/starter-kit-client/issues"
  },
  "homepage": "https://github.com/cav2008/starter-kit-client#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.3.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.9",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.14.1",
    "redux-devtools": "^3.4.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "sinon": "^4.4.6",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2"
  }
}
